{
    "name": "cydrobolt/polr",
    "description": "The Polr URL Shortener.",
    "keywords": ["url-shortener", "url", "cms"],
    "license": "GPLv2+",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "8.0.*",
        "vlucas/phpdotenv": "~5.3.0",
        "illuminate/mail": "~8.27",
        "yajra/laravel-datatables-oracle": "~9.15.1",
        "paragonie/random_compat": "^2.0.19",
        "torann/geoip": "^3.0.2",
        "geoip2/geoip2": "^2.11.0",
        "nesbot/carbon": "^2.45.1",
        "doctrine/dbal": "3.0.0",
        "google/recaptcha": "~1.2.4",
        "symfony/http-foundation": "5.2.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.0",
        "phpunit/phpunit": "^9.5.2",
        "symfony/css-selector": "^5.2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
